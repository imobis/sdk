{
    "name": "imobis/api-sdk",
    "description": "IMOBIS API SDK",
    "keywords": [
        "imobis",
        "api-sdk"
    ],
    "homepage": "https://github.com/imobis/api-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Kislitsa",
            "email": "igor.k@corp.mmngr.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || >=8.0 <8.4",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^2.10 || ^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Nexus\\Message\\Sdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nexus\\Message\\Sdk\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
